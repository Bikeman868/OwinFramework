<?xml version="1.0"?>
<package >
  <metadata>
    <id>Owin.Framework.Urchin</id>
    <version>2.7.1</version>
    <title>Owin.Framework.Urchin</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configure Owin Framework middleware using Urchin</description>
    <copyright>Copyright Martin Halliday 2014-2020</copyright>
    <releaseNotes>For use with Owin Framework of the same version number</releaseNotes>
    <summary>
      Configures an implementation of IConfiguration in your IoC container
      that will use the Urchin rules-based configuration management system to 
      configure Owin middleware.
    </summary>
    <projectUrl>https://github.com/Bikeman868/OwinFramework/wiki</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>OWIN Owin-framework urchin</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Owin" version="1.0" />
        <dependency id="Microsoft.Owin" version="2.1.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Urchin.Client" version="1.3.0" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
      <group targetFramework="net45">
        <dependency id="Owin" version="1.0" />
        <dependency id="Microsoft.Owin" version="3.1.0" />
        <dependency id="Ioc.Modules.Contract" version="1.4.1" />
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Urchin.Client" version="1.3.0" />
        <dependency id="Newtonsoft.Json" version="7.0.1" />
      </group>
    </dependencies>
    <references>
    </references>
  </metadata>
  <files>
    <file src="..\OwinFramework.Configuration.Urchin\bin\Release\net40\OwinFramework.Urchin.dll" target="lib\net40"/>
    <file src="..\OwinFramework.Configuration.Urchin\bin\Release\net45\OwinFramework.Urchin.dll" target="lib\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>