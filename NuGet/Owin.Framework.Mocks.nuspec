<?xml version="1.0"?>
<package >
  <metadata>
    <id>Owin.Framework.Mocks</id>
    <version>2.7.1</version>
    <title>Owin.Framework.Mocks</title>
    <authors>Martin Halliday</authors>
    <owners>Martin Halliday</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Mocks of Owin Framework interfaces to use in your unit tests</description>
    <copyright>Copyright Martin Halliday 2014-2020</copyright>
    <releaseNotes>For use with Owin Framework of the same version number</releaseNotes>
    <summary>
      Uses Moq.Modules to provide a set of plug-in Mocks that can seamlessly
      integrate into your unit tests. These are useful if you are writing
      Middleware that uses the facilities defined by Owin Framework interfaces.
    </summary>
    <projectUrl>https://github.com/Bikeman868/OwinFramework/wiki</projectUrl>
    <license type="expression">Apache-2.0</license>
    <tags>OWIN Owin-framework moq modules mock</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Moq.Modules" version="1.0.2" />
        <dependency id="NUnit" version="3.5.0" />
      </group>
      <group targetFramework="net45">
        <dependency id="Owin.Framework" version="2.7.1" />
        <dependency id="Moq.Modules" version="1.0.2" />
        <dependency id="NUnit" version="3.5.0" />
      </group>
    </dependencies>
    <references>
    </references>
  </metadata>
  <files>
    <file src="..\OwinFramework.Mocks\bin\Release\net40\OwinFramework.Mocks.dll" target="lib\net40"/>
    <file src="..\OwinFramework.Mocks\bin\Release\net45\OwinFramework.Mocks.dll" target="lib\net45"/>
    <file src="..\*.md" target=""/>
  </files>
</package>